label live-@FLAVOUR@
	menu label ^Live (@FLAVOUR@)
	menu title Live (@FLAVOUR@)
	menu default
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE@

label live-@FLAVOUR@-failsafe
	menu label ^Live (@FLAVOUR@ failsafe)
	menu title Live (@FLAVOUR@ failsafe)
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE_FAILSAFE@
